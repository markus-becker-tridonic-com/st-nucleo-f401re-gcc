{
  "name": "st-nucleo-f401re-gcc",
  "version": "0.1.0",
  "inherits": {
    "mbed-gcc": "*"
  },
  "description": "Official mbed build target for the mbed st-nucleo-f401re development board.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "mbed-target:nucleo-f401re",
    "mbed-official",
    "nucleo-f401re",
    "f401",
    "nucleo",
    "gcc"
  ],
  "similarTo": [
    "nucleo-f401re",
    "f401re",
    "f401",
    "nucleo",
    "stm32f401ret6",
    "stm32f401re",
    "stm32f401xe",
    "stm32f401",
    "stm32f4",
    "stm32",
    "st",
    "cortex-m4",
    "armv7-m",
    "arm",
    "gcc",
    "mbed"
  ],
  "config": {
    "mbed-os": {},
    "cmsis": {
      "nvic": {
        "ram_vector_address": "0x20000000",
        "flash_vector_address": "0x08000000",
        "user_irq_offset": 16,
        "user_irq_number": 101
      }
    },
    "hardware": {
      "externalClock": "8000000",
      "pins": {
        "LED1": "PA_5",
        "LED2": "NC",
        "LED3": "NC",
        "LED4": "NC",
        "USER_BUTTON": "PC_13",
        "SW2": "NC",
        "SW3": "NC",
        "SERIAL_TX": "PA_2",
        "SERIAL_RX": "PA_3",
        "USBTX": "PA_2",
        "USBRX": "PA_3",
        "I2C_SCL": "PB_8",
        "I2C_SDA": "PB_9",
        "SPI_MOSI": "PA_7",
        "SPI_MISO": "PA_6",
        "SPI_SCK": "PA_5",
        "SPI_CS": "PA_4",
        "PWM_OUT": "PB_3"
      },
      "test-pins": {
        "spi": {
          "mosi": "SPI_MOSI",
          "miso": "SPI_MISO",
          "sclk": "SPI_SCK",
          "ssel": "SPI_CS"
        },
        "i2c": {
          "sda": "I2C_SDA",
          "scl": "I2C_SCL"
        },
        "serial": {
          "tx": "PC_12",
          "rx": "PF_6"
        }
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f401re",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "stm32f401re",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "NUCLEO_F401RE",
      "$program"
    ]
  }
}
